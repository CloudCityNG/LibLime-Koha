var closeGB = '<!-- TMPL_VAR NAME="closeGB" -->';
if (closeGB=='1') { // confirmed filled from Holds Queue
   parent.parent.document.location.href = '/cgi-bin/koha/circ/view_holdsqueue.pl'
   + '?branchlimit=<!-- TMPL_VAR NAME="queue_branchlimit" -->'
